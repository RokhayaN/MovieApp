<h2>Edit Movie </h2>

<%= form_for @movie do |f| %>
<%= f.label :title %>
<%= f.text_field :title %><br><br>
<%= f.label :image %>
<%= f.image_tag :image%><br><br>
<%= f.label :genre %>
<%= f.text_field :genre %><br><br>
<%= f.label :release_year %><br>
<%= f.text_area :release_year%><br><br>
<%= f.submit "Update Movie "%>
<% end %>