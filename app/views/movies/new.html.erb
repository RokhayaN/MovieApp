<h2>Add a New Movie </h2>

<%= render partial: '/errors', locals: { record: @movie } %>


<%= form_for @movie do |f| %>

<%= f.label :title %>
<%= f.text_field :title %><br><br>
<%= f.label :image %>
<%= f.text_field :image %><br><br>
<%= f.label :genre %>
<%= f.text_field :genre %><br><br>
<%= f.label :release_year %><br>
<%= f.text_field :release_year%><br><br>
<%= f.submit "Create New Movie" %>
<% end %>